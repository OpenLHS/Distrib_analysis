sigmoid <- function(x) {
exp(x) / (1 + exp(x))
}
logreg_D <- function(beta, X, y, W) {
n <- nrow(X)
t(X) %*% W %*% (y - sigmoid(X %*% beta)) # / n
}
logreg_V <- function(beta, X, W) {
n <- nrow(X)
sig <- sigmoid(X %*% beta)[,1]
t(X) %*% W %*% diag(sig*(1-sig)) %*% X # / n
}
